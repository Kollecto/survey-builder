%tr
  %td= render 'admin/survey_options/details_cell', :option => so
  -# %td= render 'admin/survey_questions/details_cell',
  -#             :question => @question || so.survey_question
  -# %td= render 'admin/survey_pages/details_cell',
  -#             :page => @page || so.survey_page
  %td= render 'admin/survey_iterations/details_cell',
              :iteration => @iteration || so.survey_iteration
  %td.text-center
    = link_to [:edit, :admin, so], title: 'Edit Option',
      class: 'btn btn-xs btn-default has-tooltip edit-btn' do
      %span.glyphicon.glyphicon-edit
    = link_to [:admin, so], title: 'Delete Option', method: :delete,
      class: 'btn btn-xs btn-default has-tooltip delete-btn',
      data: { confirm: 'Are you sure?' } do
      %span.glyphicon.glyphicon-trash
